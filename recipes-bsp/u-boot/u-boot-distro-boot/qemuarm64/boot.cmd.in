# Torizoncore boot script.

if test -z "${altbootcmd}"
then
    env set altbootcmd 'env set rollback 1; run bootcmd'
    env save
fi

ext4load ${devtype} ${devnum}:1 ${scriptaddr} /boot/loader/uEnv.txt; env import -t ${scriptaddr} ${filesize}
run bootcmd_run

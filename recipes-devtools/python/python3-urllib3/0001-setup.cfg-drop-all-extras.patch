 setup.cfg | 3 ---
 1 file changed, 3 deletions(-)

diff --git a/setup.cfg b/setup.cfg
index 81b5727..04dd58c 100644
--- a/setup.cfg
+++ b/setup.cfg
@@ -9,9 +9,6 @@ universal = 1
 [metadata]
 license_file = LICENSE.txt

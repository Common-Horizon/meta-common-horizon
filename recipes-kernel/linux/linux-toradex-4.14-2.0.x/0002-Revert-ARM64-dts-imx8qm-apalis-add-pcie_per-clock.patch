Subject: [PATCH 2/2] Revert "ARM64: dts: imx8qm-apalis: add pcie_per clock"

This reverts commit 81d5c3f7cc7490bde7d388a1a4a23fc38c5b8cae.
---
 arch/arm64/boot/dts/freescale/fsl-imx8qm-apalis.dtsi | 6 ++----
 1 file changed, 2 insertions(+), 4 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-imx8qm-apalis.dtsi b/arch/arm64/boot/dts/freescale/fsl-imx8qm-apalis.dtsi
index 2b0782e111c2..83a939a9748a 100644
--- a/arch/arm64/boot/dts/freescale/fsl-imx8qm-apalis.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-imx8qm-apalis.dtsi
@@ -1154,10 +1154,9 @@
 	clocks = <&clk IMX8QM_HSIO_PCIE_A_MSTR_AXI_CLK>,
 		 <&clk IMX8QM_HSIO_PCIE_A_SLV_AXI_CLK>,
 		 <&clk IMX8QM_HSIO_PHY_X2_PCLK_0>,
